{
    "extends": "eslint:recommended",
    "env": {
        "node": true,
        "es6": true
    },
    "parserOptions": {
        "ecmaVersion": 8 /* async/await */
    },
    "rules": {
        "strict": 0,
        "semi": [
            "error",
            "always"
        ],
        "no-octal": 0
    }
}
